{
  "name": "money-quiz",
  "version": "2.0.0",
  "description": "Money Quiz WordPress Plugin with AI/ML features",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint assets/js/**/*.js",
    "lint:fix": "eslint assets/js/**/*.js --fix",
    "analyze": "webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "quiz",
    "ai",
    "ml"
  ],
  "author": "The Synergy Group AG",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.21.0",
    "@babel/preset-react": "^7.18.6",
    "@wordpress/babel-preset-default": "^7.10.0",
    "@wordpress/scripts": "^25.0.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.35.0",
    "eslint-config-wordpress": "^2.0.0",
    "jest": "^29.4.3",
    "mini-css-extract-plugin": "^2.7.2",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^6.24.0",
    "@wordpress/components": "^23.3.0",
    "@wordpress/data": "^8.3.0",
    "@wordpress/element": "^5.3.0",
    "@wordpress/i18n": "^4.26.0",
    "axios": "^1.3.4",
    "chart.js": "^4.2.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-admin": "^4.8.2",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/js/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "**/tests/js/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "assets/js/**/*.js",
      "!assets/js/**/*.min.js",
      "!**/node_modules/**"
    ]
  }
}