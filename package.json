{
  "name": "money-quiz",
  "version": "7.0.0",
  "description": "Money Quiz WordPress Plugin - Frontend Assets",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "node tests/performance/frontend-bench.js",
    "lint": "eslint assets/js/",
    "lint:fix": "eslint assets/js/ --fix",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "clean": "rm -rf dist/",
    "pre-commit": "npm run lint && npm run test"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "quiz",
    "financial"
  ],
  "author": "The Synergy Group AG",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@wordpress/eslint-plugin": "^14.0.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.0",
    "sass": "^1.62.0",
    "sass-loader": "^13.3.0",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.87.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}