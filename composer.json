{
    "name": "thesynergygroup/money-quiz",
    "description": "A secure, enterprise-grade financial assessment quiz plugin for WordPress",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "version": "7.0.0",
    "homepage": "https://thesynergygroup.ch/money-quiz",
    "authors": [
        {
            "name": "The Synergy Group AG",
            "email": "info@thesynergygroup.ch",
            "homepage": "https://thesynergygroup.ch"
        }
    ],
    "support": {
        "issues": "https://github.com/thesynergygroup/money-quiz/issues",
        "source": "https://github.com/thesynergygroup/money-quiz"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "psr/container": "^1.0 || ^2.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "symfony/polyfill-php80": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^3.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpstan/phpstan": "^1.10",
        "szepeviktor/phpstan-wordpress": "^1.3",
        "mockery/mockery": "^1.5",
        "brain/monkey": "^2.6",
        "php-stubs/wordpress-stubs": "^6.3",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "MoneyQuiz\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MoneyQuiz\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs",
            "@phpstan"
        ],
        "phpunit": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/phpcs --standard=WordPress-Extra,WordPress-Docs -s .",
        "phpcs-fix": "vendor/bin/phpcbf --standard=WordPress-Extra,WordPress-Docs .",
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=256M",
        "security-check": "composer audit",
        "install-standards": [
            "vendor/bin/phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs"
        ],
        "post-install-cmd": [
            "@install-standards"
        ],
        "post-update-cmd": [
            "@install-standards"
        ],
        "pre-commit": [
            "@phpcs",
            "@phpstan",
            "@security-check"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "vendor/szepeviktor/phpstan-wordpress/extension.neon"
            ]
        }
    }
}